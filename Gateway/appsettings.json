{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "weatherapi": {
        "ClusterId": "weatherservice",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "weatherswagger": {
        "ClusterId": "weatherservice",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      },
      "weatherswagger2": {
        "ClusterId": "weatherservice",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/swagger"
        }
      },
      "weatherapp": {
        "ClusterId": "weatherapp",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "weatherservice": {
        "Destinations": {
          "weatherservice-1": {
            "Address": "http://localhost:5149/"
          }
        }
      },
      "weatherapp": {
        "Destinations": {
          "weatherapp-1": {
            "Address": "http://localhost:5022/"
          }
        }
      }
    }
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "aspnetapp.pfx",
        "Password": "password"
      }
    }
  },
  "Authority": "https://localhost:5001"
}
